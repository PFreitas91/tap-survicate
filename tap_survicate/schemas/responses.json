{
    "type": "object",
    "properties": {
      "uuid": {
        "type": "string"
      },
      "respondent_uuid": {
        "type": "string"
      },
      "collected_at": {
        "type": "string"
      },
      "url": {
        "type": "string"
      },
      "device_type": {
        "type": "string"
      },
      "operating_system": {
        "type": "string"
      },
      "language": {
        "type": "string"
      },
      "questions": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "question_id": {
              "type": "integer"
            },
            "question_type": {
              "type": "string"
            },
            "answer": {
              "type": "object",
              "properties": {
                "id": {
                  "type": "integer"
                },
                "content": {
                  "type": "string"
                },
                "comment": {
                  "type": ["string", "null"]
                }
              },
              "required": ["id", "content"]
            },
            "fields": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "type": {
                    "type": "string"
                  },
                  "content": {
                    "type": "string"
                  }
                },
                "required": ["type", "content"]
              }
            }
          },
          "required": ["question_id", "question_type"]
        }
      },
      "respondent": {
        "type": "object",
        "properties": {
          "uuid": {
            "type": "string"
          },
          "attributes": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        },
        "required": ["uuid"]
      }
    },
    "required": ["uuid", "collected_at"]
  }  
